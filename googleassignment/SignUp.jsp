<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN">
<html>
  <head>
    <meta http-equiv="content-type" content="text/html; charset=UTF-8">
    <title>SignUp</title>
    <script src="jquery.js"></script>
<script src="jquery.validate.js" type="text/javascript"></script>
<script type="text/javascript">
$(document).ready(function() {
	$("#commentForm").validate();
$("#firstname").css("border","2px  solid red");
$("#lastname").css("border","2px  solid red");
$("#username").css("border","2px  solid red");
$("#password").css("border","2px  solid red");
$("#emailid").css("border","2px  solid red");
$("#repassword").css("border","2px  solid red");
$("#question").css("border","2px  solid red");
$("#answer").css("border","2px  solid red");
$("#secemail").css("border","2px  solid red");
 $("#hide").click(function () {
      $("pa").hide("fast", function () {
        // use callee so don't have to name the function
        $(this).prev().hide("fast", arguments.callee); 
      });
});
$("#showr").click(function () {
      $("pa").show(2000);
    });
});
</script>
  </head>
  <body>
    <iframe src="javascript:''" id="__gwt_historyFrame" tabIndex='-1' style="position:absolute;width:0;height:0;border:0"></iframe>
<form id="commentForm" method="GET" action="http://localhost:8090/Account/signup">
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<b><font color=green>    Create a Google Account - Gmail.com <b>
<br><br>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<pa>      This Page is Developed By Sonal Vinod Patil   ..........</pa>
<br><br><br>
<table width="50%" border="1" cellspacing="0" cellpadding="20" align="left" bordercolor="gray">
  <tr>
    <td><table width="800" border="0" cellspacing="0" cellpadding="7" >
      <tr>
        <td><div align="left"><b>First Name :</b> </div></td>
        <td><input type="text" name="firstname" id="firstname" class="required" size=30/></td>
      </tr>
      <tr>
        <td width="200"><label></label>
            <div align="left"><b>Last Name : </b></div></td>
        <td width="200"><input type="text" name="lastname" id="lastname" class="required" size=30/>
        </td>
      </tr>
      <tr>
        <td><p align="left"><b>Desired Login:</b> </p></td>
        <td><input type="text" name="username" id="username" class="required" size=30/>
    <font face="Monotype Corsiva" size="+1" color="red">@gmail.com</font></td>
      </tr>
      <tr>
        <td><p align="left"><b>Choose Password :</b> </p></td>
        <td><input type="password" name="password" id="password" class="required" size=30/></td>
      </tr>
      <tr>
        <td><div align="left"><b>Re-enter Password :</b> </div></td>
        <td><input type="password" name="repassword" id="repassword" equalTo=#password class="required" size=30/></td>
      </tr>
    <tr>
        <td><div align="left"><b>Choose Question</b></div></td>
        <td>
    <select name="question" value="question">
    <option value="First phone">Your First phone number?</option>
        <option value="Card">Your Library card number?</option>
    <option value="Teacher">What was your first teachers name?</option>
    <option value="Hero">Who is your favourite hero?</option>
    </select>
    </tr>
    <tr>
        <td><p align="left"><b>Answer : </b></p></td>
        <td><input type="text" name="answer" id="answer" class="required" size=30 /></td>
      </tr>
    <tr>
        <td><p align="left"><b>Secondary Email : </b></p></td>
        <td><input type="text" name="secemail" id="secemail" class="required" size=30/></td>
      </tr>
    <tr>
        <td><div align="left"><b>Location : </b></div></td>
        <td>
    <select name="location" value="location" id="location" class="required">
    <option value="India">India</option>
        <option value="UK">UK</option>
    <option value="USA">USA</option>
    <option value="Maxico">Maxico</option>
        <option value="France">France</option>
    <option value="Itali">Italy</option>
    <option value="Nepal">Nepal</option>
    </select>
    </tr>
      <tr>
      <br>
        <td><div align="justify">
            <input name="button" type="submit" value="  Create My Account  " id="submit">
       </td>
        <td><input name="reset" type="reset" value="   Reset   " /></div></td>
        <td><input type=button id=showr name=submit value=Show></div></td>
<td><div align="justify"></div></td>
      </tr>
    </table></td>
  </tr>
</table>

</form>
  </body>
</html>